@book{Niemi2001,
abstract = {Chapters 17 through 20 in Controversies in Voting Behavior focus on the debate surrounding the durability of partisan identification. The greatest manifestation of this debate can be understood in the incongruity between the stability of partisanship at the individual level and the instability of partisanship at the aggregate level. Miller argues that generational changes alter partisanship at the macro- level by looking at three different age cohorts. He finds that the eldest cohort utilizes Fiorina's retrospective voting most readily, and the younger cohort shifts from non-partisans towards partisan ones with political events. Erikson, MacKuen, and Stimson's ‘macropartisanship' states that the proportion of partisans, D/D+R, is a reflection of consumer sentiment and presidential approval. Green, Palmquist, and Schickler counter the ‘macropartisanship' claim by arguing that one would need to observe truly massive shifts in presidential approval or economic indicators to produce even slight changes in the levels of partisanship.},
author = {Niemi, Richard G. and Weisberg, Herbert F.},
booktitle = {Controversies in Voting Behavior},
isbn = {0-87187-706-6},
pages = {22--37},
title = {{Controversies in Voting Behavior}},
year = {2001}
}
@article{Munshi2003,
abstract = {This paper attempts to identify job networks among Mexican migrants in the U. S. labor market. The empirical analysis uses data on migration patterns and labor market outcomes, based on a sample of individuals belonging to multiple origin-communities in Mexico, over a long period of time. Each community's network is measured by the proportion of the sampled individuals who are located at the destination (the United States) in any year. We verify that the same individual is more likely to be employed and to hold a higher paying nonagricultural job when his network is exogenously larger, by including individual fixed effects in the employment and occupation regressions and by using rainfall in the origin-community as an instrument for the size of the network at the destination.},
author = {Munshi, Kaivan},
