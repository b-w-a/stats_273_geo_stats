file = {:Users/bryanwilcox/Dropbox/readings/articles/social{\_}psych/kang{\_}etal{\_}2015.pdf:pdf},
isbn = {1545-2085 (Electronic)$\backslash$r0066-4308 (Linking)},
issn = {1545-2085},
journal = {Annual review of psychology},
keywords = {crossed categorization,discrimination,diversity,duplicate,person perception,social cognition,social identity},
