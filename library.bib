@article{Nuno2007,
abstract = {Previous scholarship on Latino politics has demonstrated that mobilization has a statistically significant effect on voter turnout, suggesting the importance of get-out-the-vote campaigns to increase Latino political participation. Although nonpartisan organizations exist to mobilize Latino voters, most of the phone calls are made by political parties and candidates. I argue that the real test of effectiveness for partisan mobilization is vote choice. Using data from the 2000 presidential election, I model vote choice to determine whether or not Latinos who were contacted by Democrats and Republicans were more likely to vote for Al Gore and George W. Bush. I find that party mobilization by other Latinos is crucial to influencing vote choice. Specifically, Latinos who were contacted by Latino Republicans were significantly more likely to prefer Bush, whereas Latinos who were contacted by non-Latino Republicans were significantly less likely to prefer Bush. Several models are explored. {\textcopyright} 2007 Sage Publications.},
author = {Nuno, S. a.},
doi = {10.1177/1532673X06297032},
file = {:Users/bryanwilcox/Library/Application Support/Mendeley Desktop/Downloaded/Nuno - 2007 - Latino Mobilization and Vote Choice in the 2000 Presidential Election.pdf:pdf},
isbn = {1532-673X},
issn = {1532-673X},
journal = {American Politics Research},
keywords = {8 americans are of,and latinos now,compose the largest minority,latino background,latino voters,ore than 1 in,political mobilization,political participation,population in the united,presidential elections,states,the,voting},
pages = {273--293},
title = {{Latino Mobilization and Vote Choice in the 2000 Presidential Election}},
volume = {35},
year = {2007}
}
