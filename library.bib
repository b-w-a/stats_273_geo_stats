journal = {Behavior genetics},
keywords = {Adult,Attitude,Australia,Cohort Studies,Data Collection,Health Status,Humans,Life Style,Politics,Questionnaires,Registries,Social Behavior,Social Class,Socioeconomic Factors},
month = {may},
number = {3},
pages = {435--48},
title = {{The genetics of voting: an Australian twin study.}},
volume = {37},
year = {2007}
}
