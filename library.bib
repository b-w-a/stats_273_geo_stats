@article{Grossman2015,
abstract = {We investigate the effects of government fragmentation on the quality of public ser- vices delivery. We argue that an increase in the number of regional governments affects the quality of the pool future leaders are drawn from, and encourages yardstick com- petition. Extreme government fragmentation though limits efficiency gains by diluting informational signals about leader quality, by reducing administrative capacity, reduc- ing economies of scale, and eliciting elite capture. Initial gains will be concentrated in regions directly affected by changes in territorial structures. We test our theoreti- cal argument using original data on the number of top tier regional governments for Sub-Saharan Africa (1960-2012). Consistent with our theoretical expectations, we find robust evidence for an initial increase in the quality of services provision and, to a lesser extent, a decrease in quality at very high levels of regional governments fragmentation. We then further test our theory with three distinct difference-in-difference analyses of micro-level, geo-referenced data on health outcomes in Malawi, Nigeria, and Uganda. Consistent with out expectations, we show that health outcomes improve first in ‘splin- ter' regions after administrative boundary reforms, while ‘mother' regions have to cope with a temporary loss of governmental resources.},
author = {Grossman, Guy and Pierskalla, Jan H and Dean, Emma Boswell},
file = {:Users/bryanwilcox/Dropbox/readings/articles/geography{\_}context/grossman{\_}PierskallaBoswell{\_}April2015.pdf:pdf},
