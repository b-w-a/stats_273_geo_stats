@article{LevyPaluck2010,
abstract = {Randomized field experiments should take a more central place in qualitative research. Although field experimentation is often considered a quantitative enterprise, this paper illustrates the compatibility of field experimentation with various types of qualitative measurement tools and research questions. Integrating qualitative and quantitative data within field experiments allows investigators to move past simple average treatment effects and explore mechanisms of the identified cause and effect. A more novel proposal is to use field experimentation as the organising methodological framework for archival, ethnographic, or interpretative work, and to use ethnographic methods as the primary source of measurement in "experimental ethnography". Sustained research and theoretical specificity can address some of the seemingly incompatible features of qualitative and field experimental methods. For example, small sample sizes are acceptable as part of a research program, and some theories of historical patterns or rare event could be disaggregated into smaller cause-and-effect linkages to test with field experiments in theoretically relevant contexts.},
author = {Paluck, EL},
doi = {10.1177/0002716209351510},
file = {:Users/bryanwilcox/Library/Application Support/Mendeley Desktop/Downloaded/Paluck - 2010 - The promising integration of qualitative methods and field experiments.pdf:pdf},
