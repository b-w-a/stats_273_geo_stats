@article{Jackson1987,
abstract = {This article explores the relationship between ethnic group identity and political behavior among black Americans in a midwestern city. Past studies of electoral behavior among blacks have suggested that the Black Power Movement brought about racial cohesion among blacks in urban areas whereby black Americans moved toward recruiting and supporting candidates of their own race for public office as opposed to supporting candidates of "Political Machines" or the "Establishment." The elections of Mayor Harold Washington in Chicago and Wilson Goode of Philadelphia would reconfirm this conviction. However, the research findings in this analysis point to the complexity of coalition formation in urban black communities. The findings suggest that (1) black support for black candidates running for public office against non-blacks is not automatic and (2) "politicization" of the black electorate is a necessary condition for black voter mobilization and group cohesion. These findings generally support earlier research by Miller, Gurin et al. on group identity and political mobilization.},
author = {Jackson, Byran O.},
doi = {10.2307/448253},
issn = {0043-4078},
journal = {The Western Political Quarterly},
number = {4},
pages = {631--646},
title = {{The Effects of Racial Group Consciousness on Political Mobilization in American Cities}},
url = {http://www.jstor.org/stable/448253{\%}5Cnhttp://www.jstor.org.proxy.lib.ohio-state.edu/stable/pdfplus/448253.pdf?acceptTC=true{\%}5Cnhttp://search.ebscohost.com/login.aspx?direct=true{\&}db=ssa{\&}AN=511662073{\&}site=ehost-live},
volume = {40},
year = {1987}
}
@article{Perez2013,
abstract = {"Political elites often discuss racial/ethnic outgroups in a critical light. I claim this discourse raises the salience of group identity while impugning its worth, thus inducing differential political reactions among high and low identifying group members. Specifically, high identifiers will engage in political efforts that restore their identity's positive value by displaying ingroup favoritism and challenging the source of their group's devaluation. In contrast, low identifiers will actively decline political opportunities to bolster their group's devalued status. Using a national survey experiment, I randomly assigned eligible but unregistered Latino voters to a control group without elite discourse; a non-devaluing condition with elite discourse focused on illegal immigration; or, a devaluing condition with elite discourse focused on illegal immigration and critical of illegal immigrants. High identifying Latinos in the devaluing condition expressed greater pro-Latino political attitudes and a stronger intention to register and vote in a pending presidential election. This dynamic was absent in the other conditions and unrelated to Latinos' partisan identity. These results suggest an identity-to-politics link is robustly forged among high identifying group members when they sense a devaluation of their group."},
author = {P{\'{e}}rez, Efr{\'{e}}n O.},
doi = {10.1007/s11109-013-9262-0},
isbn = {0190-9320$\backslash$r1573-6687},
issn = {01909320},
journal = {Political Behavior},
keywords = {Latino politics,Racial and ethnic politics,Social identity theory (SIT),Survey experiments},
number = {1},
pages = {155--180},
title = {{Ricochet: How Elite Discourse Politicizes Racial and Ethnic Identities}},
volume = {37},
year = {2015}
}
@article{Shingles1981,
abstract = {Recent research has demonstrated that black Americans are far more politically active than whies of similar socioeconomic status. The difference has been related to black consciousness. Yet the reasons for this relationship have not been adequately explained. Starting with the work of Gurin and Gamson, this article theorizes taht black consciousness contributes to political mistrust and a sense of internal political efficacy which in turn encourages policy-related participation. The relationshiop between the two attitudes and policy-related behavior is demonstrated to be conditional. The conditions favor blacks more than whites. What I shall call the Gamson-Gurin thesis is supported by data from Verba's and Nie's 1967 survey of the American public. The thesis, and its derivations, prove useful in clarifying the scope and nature of black participation in the American political process as well as helping us to understand how individuals in general select one mode of participation over another and how the choice varies by race and social class.},
author = {Shingles, Richard D .},
doi = {10.2307/1962160},
file = {:Users/bryanwilcox/Dropbox/readings/articles/social{\_}psych/identity{\_}formation/shingles{\_}1981.pdf:pdf},
